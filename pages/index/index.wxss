/**index.wxss**/
.button-sp-area{
    margin: 0 auto;
    padding-top: 15px;
    width: 60%;
}
.mini-btn{
    margin-right: 5px;
}

.index-line{
  margin-top:10px;
  border: 2rpx solid #000;
}
.page__bd{
  display: flex;
  flex-direction: row;
}
.header_bd{
  width: 100%;
    height: 200px;
    background: url('data:image/jpeg;base64,') no-repeat;
    background-size: 100% 100%;
    text-align: center;
}

.lunbo{
  flex:1;
  margin:0 20px;
  align-items: center;
  justify-content: center;
  border-radius:15px;
  color:#fff;
  background: rgba(0, 0, 0, 0.4);
  font-size: 15px;
  padding:5px 0;
}
.title{
  padding:10px;
}

.panel{
  background-color: #fff;
  height: 128px;
  border-radius: 10px;
  margin: -70px 20px 0 20px;
  color: #3c8df9;
  font-size: 13px;
  text-align: center;
  
}
.panel-box{
  display: flex;
  flex-direction: row;
}
.panel-title{
  padding:10px;
}
.panel-box-item{
  flex: 1;
}

.panel-box-item-grey{
    flex: 1;
    color:#C2C2C2;
}

.message{
  font-size: 15px;
  display: flex;
  flex-direction: row;
  padding:10px 15px;
  background: #fff;
  margin: 10px 0 1px 0;
}

.my-workorder{
  font-size: 15px;
  display: flex;
  flex-direction: row;
  padding:10px 15px;
}

.message-title{
  flex: 1;
  text-align: right
}
.message-detail{
  padding:5px 15px;
  background: #fff;
  display: flex;
  flex-direction: row;
  font-size: 14px
}
.line{
  border: 1px solid;
  color: #F0F0F0;
  margin:10px 0;
}
.work-list{
  background:#f7f7f7; 
  height: 200px;
}

.work-item{
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
  margin: 0 10px 10px 10px;
  border-radius: 15px;
  padding:20rpx;
  background:#fff; 
}

.view-shadow{
  box-shadow: 0 0 10px 0 rgba(0,0,0,0.05);
}
.content-style{
  font-size:14px;margin:0 10px;padding:1px
}

.order-type{
  font-size: 28px;
  background: #8CDCC4;
  border-radius: 50%; 
  color: #fff;
  width: 50px;
  height: 50px;
  text-align: center;
}
.order-type-out{
  font-size: 28px;
  background: #A0CEF0;
  border-radius: 50%; 
  color: #fff;
  width: 50px;
  height: 50px;
  text-align: center;
}
.order-type-search{
  font-size: 28px;
  background: #00EE76;
  border-radius: 50%; 
  color: #fff;
  width: 50px;
  height: 50px;
  text-align: center;
}
.date-style{
  color: #C7C7C7;
  font-size: 12px;
  text-align: right;
  margin-right: 15px;
}
